{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_source",
						"type": "DatasetReference"
					},
					"name": "dfSource"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          airline_id as string,",
				"          name as string,",
				"          alias as string,",
				"          iata as string,",
				"          icao as string,",
				"          callsign as string,",
				"          country as string,",
				"          active as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> dfSource"
			]
		}
	}
}